{
  "name": "suno-analyzer",
  "version": "1.0.0",
  "type": "module",
  "main": "app.js",
  "scripts": {
    "start": "node dist/app.js",
    "build": "tsc && npm run copy:public",
    "copy:public": "npx cpx2 \"src/public/**/*\" dist/public --clean",
    "dev:copy": "cpx2 \"src/public/monitoring/**/*\" dist/public/monitoring --watch",
    "dev": "concurrently -n BACK,FRONT -c green,blue \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "tsc-watch --onSuccess \"nodemon dist/app.js --ignore dist/public/**\" --noClear",
    "dev:frontend": "cd monitoring-ui && npm run dev"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.0",
    "@visx/visx": "^3.12.0",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "nodemon": "^3.1.9",
    "playwright": "^1.52.0",
    "redis": "^4.7.0",
    "swagger-ui-express": "^5.0.1",
    "ws": "^8.18.1",
    "zod": "^3.24.3",
    "cpx2": "^8.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.1",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/ws": "^8.18.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4",
    "tsc-watch": "^6.2.1",
    "typescript": "^5.8.3"
  }
}