{
  "name": "suno-proxy",
  "version": "1.1.00",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "staging": "cross-env NODE_ENV=staging node dist/server.js",
    "build": "tsc && npm run copy:public",
    "copy:public": "npx cpx2 \"src/public/**/*\" dist/public --clean",
    "dev": "cross-env NODE_ENV=development tsc-watch --onSuccess \"nodemon dist/server.js --ignore dist/public/**\" --noClear",
    "test": "cross-env NODE_ENV=test vitest --run",
    "test:ui": "cross-env NODE_ENV=test vitest --ui"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.0",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "cpx2": "^8.0.0",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "mongodb": "^6.16.0",
    "nodemon": "^3.1.9",
    "playwright": "^1.52.0",
    "redis": "^4.7.0",
    "swagger-ui-express": "^5.0.1",
    "ws": "^8.18.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.1",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/ws": "^8.18.1",
    "@vitest/ui": "^3.1.3",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "supertest": "^7.1.0",
    "tsc-watch": "^6.2.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  }
}